<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>ZhangWuServlet</servlet-name>
        <servlet-class>gjp.controller.ZhangWuServlet</servlet-class>

    </servlet>
    <servlet-mapping>
        <servlet-name>ZhangWuServlet</servlet-name>
        <url-pattern>/ZhangWuServlet</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>gjp.controller.LoginServlet</servlet-class>

    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>ServletAll</servlet-name>
        <servlet-class>gjp.controller.ServletAll</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletAll</servlet-name>
        <url-pattern>/ServletAll</url-pattern>
    </servlet-mapping>

</web-app>